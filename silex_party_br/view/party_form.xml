            <newline />
            <label name="cpf"/>
            <field name="cpf"/>
            <label name="rg"/>
            <field name="rg"/>
            <label name="emissor"/>
            <field name="emissor"/>
            <label name="data_emissao"/>
            <field name="data_emissao"/>
            <newline />
            <label name="cnpj"/>
            <field name="cnpj"/>
            <label name="ie"/>
            <field name="ie"/>
            <label name="im"/>
            <field name="im"/>
        </page>
    </xpath>
</data>